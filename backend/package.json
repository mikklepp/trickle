{
  "name": "trickle-backend",
  "version": "0.1.0",
  "description": "Trickle backend Lambda functions",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc && cp package.json package-lock.json dist/ && npm install --omit=dev --prefix dist/",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.927.0",
    "@aws-sdk/client-dynamodb": "^3.917.0",
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/client-scheduler": "^3.927.0",
    "@aws-sdk/client-sesv2": "^3.917.0",
    "@aws-sdk/client-ssm": "^3.918.0",
    "@aws-sdk/lib-dynamodb": "^3.917.0",
    "jsonwebtoken": "^9.0.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.156",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.0",
    "typescript": "^5.9.3"
  }
}
