{
  "name": "trickle-cdk",
  "version": "1.0.0",
  "description": "Trickle CDK Infrastructure",
  "private": true,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc && cdk synth",
    "watch": "tsc -w",
    "cdk": "cdk",
    "diff": "cdk diff --all",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all --require-approval=never",
    "destroy": "cdk destroy --all"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "aws-cdk": "^2.167.0",
    "dotenv": "^17.2.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.917.0",
    "aws-cdk-lib": "^2.167.0",
    "constructs": "^10.3.0"
  }
}
